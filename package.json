{
  "name": "svkit-portfolio",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --plugin-search-dir=. .",
    "test:open": "cypress open",
    "test:run": "cypress run --env type=actual --browser electron",
    "test:snapshot": "cypress run --env type=actual --browser electron --spec cypress/e2e/regression.cy.ts",
    "test:update": "cypress run --env type=base --browser electron --spec cypress/e2e/regression.cy.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-vercel": "^5.4.5",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/nodemailer": "^6.4.9",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.11.0",
    "cypress-visual-regression": "^3.0.0",
    "nodemailer": "^6.9.4",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^5.0.0"
  },
  "type": "module"
}
